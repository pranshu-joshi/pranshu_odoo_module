    <!-- Pivot view of cars -->
     <record id="view_cars_pivot" model="ir.ui.view">
        <field name="name">view.cars.pivot</field>
        <field name="model">automotives.cars</field>
        <field name="arch" type="xml">
            <pivot>
                <field name="company_id" type="row"/>
                <!-- day, week, month, quarter and year can be used in interval-->
                <field name="dom" type="row" interval="quarter"/>
                <field name="dom" type="row" interval="month"/>
                <field name="brand" type="col"/>
                <field name="price" type="measure"/>
            </pivot>
        </field>
    </record>

     <!-- Kanban view of Student -->
    <record id="view_cars_kanban" model="ir.ui.view">
        <field name="name">view.cars.kanban</field>
        <field name="model">automotives.cars</field>
        <field name="arch" type="xml">
            <kanban>
                <field name="color"/>
                <templates>
                    <t t-name="kanban-box">
                        <div t-attf-class="#{!selection_mode ? kanban_color(record.color.raw_value) : ''}
                                            oe_kanban_card oe_kanban_global_click">
                            <div class="name_styles">
                                    <field name="name"/>
                            </div>
                            <div class="float-container">
                                <div class="cars_styles float-child">
                                    <field name="company_id"/>
                                    <br/>
                                    <field name="price"/>
                                    <div>
                                        <h3 style="color:#730b1d">
                                            <field name="comments"/>
                                        </h3>
                                    </div>
                                </div>
                                <div class="name_styles float-child">
                                    <!-- Adding an image -->
                                    <field name="car_img" widget="image" class="img"/>
                                </div>
                            </div>
                             <div class="o_dropdown_kanban o_dropdown">
                                <!-- Getting an icon of elipsis (three dots) to have the dropdown menu -->
                                <a class="dropdown-toggle o-no-caret btn" role="button"
                                   data-toggle="dropdown" data-display="static" href="#"
                                   aria-label="Dropdown menu" title="Dropdown menu">

                                    <span class="fa fa-ellipsis-v"/>
                                </a>
<!--                            Here is the dropdown list and the features contained by the dropdown-->
                                <div class="dropdown-menu" role="menu">
                                    <!-- This is for opening the record -->
                                    <a role="menuitem" type="open" class="dropdown-item">Open</a>
                                    <!-- This is for opening the record in the edit mode -->
                                    <t t-if="widget.editable"><a role="menuitem" type="edit" class="dropdown-item">Edit</a></t>
                                    <!-- This is for deleting the record -->
                                    <t t-if="widget.deletable"><a role="menuitem" type="delete" class="dropdown-item">Delete</a></t>
                                    <!-- Using Anchor tag as button of action type to call an action -->
                                    <a role="menuitem" type="action" name="%(action_feat)d" class="dropdown-item">Features</a>
                                    <!-- Using Anchor tag as button of object type to call a method -->
                                    <a role="menuitem" type="object" name="write_feature" class="dropdown-item">Update</a>
                                    <!-- Adding a seperator -->
                                    <div role="separator" class="dropdown-divider"/>
                                    <!-- Color Picker on the Kanban view -->
                                    <ul class="oe_kanban_colorpicker" data-field="color"/>
                                </div>
                             </div>
                        </div>
                    </t>
                </templates>
            </kanban>
        </field>
    </record>



