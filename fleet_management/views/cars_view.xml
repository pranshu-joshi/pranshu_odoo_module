    <record id="view_cars_form" model="ir.ui.view">
        <field name="name">view.fleet_management.form</field>
        <field name="model">automotives.cars</field>
        <field name="arch" type="xml">
<!--            <form create="0" edit="0" delete="0">-->
            <form>
                <header>
                   <field name="state" widget="statusbar" options="{'clickable':'1'}"/>
                </header>
                <sheet>
                    <div class="oe_button_box" name="button_box">
                    <button name="%(action_cars)d" string='Records' type="action" class="oe_stat_button" icon="fa-table"/>
                    <button name="%(action_features)d" type="action" class="oe_stat_button"
                            icon="fa-user"
                            context="{'search_default_Model':active_id}">
                            <field name="count_rec1" widget="statinfo"/>
                    </button>
                    </div>
                    <widget name="web_ribbon" title="Favourite" bg_color="bg-info" attrs="{'invisible': [('brand', '!=', 'tata')]}"/>
                    <widget name="web_ribbon" title="Ready" bg_color="bg-success" attrs="{'invisible': [('state', '!=', 'ready')]}"/>
                    <widget name="web_ribbon" title="Testing" bg_color="bg-warning" attrs="{'invisible': [('state', '!=', 'test')]}"/>
                    <widget name="web_ribbon" title="Archived" bg_color="bg-danger" attrs="{'invisible': [('available', '=', True)]}"/>
                    <label for="name"/>
                        <div class="name_styles">
                            <field name="name" placeholder="Car's Name"/>
                        </div>
                        <field name='car_img' widget="image" class="oe_avatar" nolabel="1"/>
                    <separator string=""/>
                    <group colspan="4" col="4">
<!--                         <label for="name"/>-->
<!--                            <h1>-->
<!--                                <field name="name" placeholder="Car's Name"/>-->
<!--                            </h1>-->
<!--                        <field name="new_rec_count"/>-->
                        <field name="car_id" required="1"/>
                        <field name="available" attrs="{'invisible': [('brand', '!=', 'tata')]}"/>
                        <field name="dom"/>
                        <field name="comments"/>
<!--                        <field name="template"/>-->
                        <field name="brand" class="cars_styles"/>
                        <label for="price" string="     "/>
                        <field name="price" nolabel="1" placeholder="Price of car"/>
<!--                        <field name="onlyfour"/>-->
                        <field name="active" invisible="1"/>
                        <field name="password" password="1"/>
<!--                        <field name="email" widget="email"/>-->
<!--                        <field name="url" widget="url"/>-->
                        <field name="priority" widget="priority"/>
<!--                        <field name="sign_in"/>-->
<!--                        <field name="sign_out" widget="float_time"/>-->
                        <field name="condition" widget="radio"
                        />
                        <field name="company_id">
                            <form>
                                <group>
                                    <field name="name" required="1"/>
<!--                                        <field name="code"/>-->
<!--                                        <field name="child_ids"/>-->
<!--                                        <field name="company_age"/>-->
                                </group>
                            </form>
                        </field>
                        <field name="com_value"/>
<!--                        <field name="currency_id"/>-->
                        <field name="extra_info"/>
                        <field name="params_store" filename="store_fname" />
                        <field name="total_over_fet"/>
                        <field name="total_avg_fet"/>
                        <field name="total_percent_fet" widget="percentpie"/>
                        <field name="store_fname" invisible="1" />

                        <field name="parent_id" domain="[('id','!=', active_id)]"/>
<!--                        <field name="awards" attrs="{'readonly' : [('name', '=', False)]}"/>-->
                        <field name="view_comments"/>
<!--                        <field name="current_user"/>-->
                        <field name="deal_on"/>
                        <field name="deal_off"/>
                        <field name="color" widget="color_picker"/>
                    </group>
                    <separator string="Others"/>

                    <notebook>
                        <page name="pg_notes" string="Features">
                            <field name="feature_id">
                                <tree>
                                    <field name="ID"/>
                                    <field name="feature_name"/>
                                    <field name="fet_v"/>
                                    <field name="fet_p"/>
                                    <field name="sub_fet_v"/>
                                    <field name="over_fet"/>
                                </tree>
                                <form>
                                    <group col="4" colspan="4">
                                        <field name="ID"/>
                                        <field name="feature_name"/>
                                        <field name="fet_v"/>
                                        <field name="fet_p"/>
                                        <field name="sub_fet_v"/>
                                        <field name="over_fet"/>
                                    </group>
                                </form>
                            </field>
                            <button name="unlink_all" string="Unlink all" type="object"/>
                            <button name="link" string="link" type="object"/>
                            <button name="create_feature" string="Add Feature" type="object"/>
                            <button name="write_feature" string="Update Feature" type="object"/>
                        </page>
                        <page name="pg_tmpl" string="Suppliers">
                            <field name="supplier_list" widget="many2many_tags"/>
                        </page>
                        <page name="pg_tmpl" string="Countries">
                            <field name="country_list" widget="many2many_checkboxes"/>
                        </page>
                        <page name="childs" string="Childs">
                            <field name="child_ids"/>
                        </page>
                        <page name="comments" string="Comments" attrs="{'invisible' : [('view_comments', '=', False)]}">
                            <field name="comments"/>
                        </page>
                        <page name="">

                        </page>
                    </notebook>
                </sheet>
                <sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids"/>
<!--                        <field name="activity_ids"/>-->
                        <field name="message_ids"/>
                    </div>
                </sheet>
                <footer>
                    <button name="create_supplier" string="Add Supplier" type="object"/>
                    <button name="cur_user" string="Current User" type="object"/>
                    <button name="term_button" string="Terminal Recs" type="object"/>
                    <button name="del_feature" string="Delete Feature" type="object"/>
                    <button name="button_seq" string="Create Sequence" type="object"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="view_mercedes_cars_form1" model="ir.ui.view">
        <field name="name">view.mercedes.cars.form</field>
        <field name="model">automotives.cars</field>
        <field name="arch" type="xml">
            <form>
                <group colspan="4" col="4">
                    <field name="car_id"/>
                    <field name="name"/>
                    <field name="brand"/>
                    <field name="dom"/>
                </group>
            </form>
        </field>
    </record>


    <record id="view_cars_tree" model="ir.ui.view">
        <field name="name">view.fleet_management.tree</field>
        <field name="model">automotives.cars</field>
        <field name="arch" type="xml">
            <tree default_order="dom"
                  decoration-warning="state=='review'"
                  decoration-info="state=='in_repair'"
                  decoration-primary="state=='test'"
                  decoration-muted="state=='deal'"
                  decoration-success="state=='ready'">
<!--                  decoration-bf="state=='review'">-->
                <field name="available" widget="boolean_toggle"/>
                <field name="car_seq" widget="handle"/>
                <field name="car_id"/>
                <field name="name" optional="hide"/>
<!--                <field name="available"/>-->
                <field name="dom" optional="show"/>
                <field name="comments" optional="hide"/>
                <field name="company_id" optional="show"/>
                <field name="price" sum="Total Price"/>
                <field name="company_id" widget="list_activity"/>
<!--                <field name="price" avg="Total Price"/>-->
<!--                <field name="sign_in"/>-->
                <field name="state" widget="badge" decoration-success="state == 'ready'"
                       decoration-info="state == 'review'" decoration-warning="state == 'in_repair'"
                       decoration-danger="state == 'test'" decoration-primary="state == 'deal'"/>
            </tree>
        </field>
    </record>

    <record id="view_mercedes_cars_tree" model="ir.ui.view">
        <field name="name">view.mercedes.cars.tree</field>
        <field name="model">automotives.cars</field>
        <field name="arch" type="xml">
            <tree>
                <field name="car_id"/>
                <field name="name"/>
                <field name="brand"/>
                <field name="dom"/>
            </tree>
        </field>
    </record>

    <record id="view_cars_search" model="ir.ui.view">
        <field name="name">view.fleet_management.tree</field>
        <field name="model">automotives.cars</field>
        <field name="arch" type="xml">
            <search>
                <searchpanel>
                    <field name="company_id"/>
                </searchpanel>
                <field name="car_id"/>
                <field name="name"/>
                <field name="available"/>
                <field name="dom"/>
                <field name="comments"/>
                <field name="company_id"/>
                <filter name="fil_comp" string="TATA" domain="[('company_id', '=', 'TATA')]"/>
                <filter name="fil_comp" string="BMW" domain="[('company_id', '=', 'BMW')]"/>
                <filter name="fil_comp" string="Mercedes" domain="[('company_id', '=', 'Mercedes')]"/>
                <filter name="fil_dom" string="Date of Manufacture" date="dom"/>
                <filter name="grp_by_company" string="Company" context="{'group_by' : 'company_id'}"/>
                <filter name="grp_by_state" string="State" context="{'group_by' : 'state'}"/>
                <filter name="grp_by_date" string="Date" context="{'group_by' : 'dom'}"/>
            </search>
        </field>
    </record>

    <!-- Calendar view of Student -->
    <record id="view_cars_calendar" model="ir.ui.view">
        <field name="name">view.cars.calendar</field>
        <field name="model">automotives.cars</field>
        <field name="arch" type="xml">
<!--            <calendar date_start="dom">-->
<!--            <calendar date_start="deal_on" date_stop="deal_off" mode="month">-->
            <calendar date_start="deal_on" date_stop="deal_off" mode="day" event_limit="3">
                <field name="car_id"/>
                <field name="name"/>
                <field name="company_id"/>
            </calendar>
        </field>
    </record>

    <record id="view_mercedes_cars_calendar" model="ir.ui.view">
